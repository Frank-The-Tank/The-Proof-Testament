
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "out",
    "sourceMap": true,
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "target": "es6",
    "rootDir": ".",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }, "exclude":["node_modules",".vscode-test"]
}
